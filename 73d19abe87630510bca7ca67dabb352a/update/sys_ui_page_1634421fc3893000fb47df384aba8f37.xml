        <html><![CDATA[<?xml version="1.0" encoding="utf-8" ?>
<j:jelly trim="false" xmlns:j="jelly:core" xmlns:g="glide" xmlns:j2="null" xmlns:g2="null">

<g:evaluate var="jvar_output" >
  var rString = RP.getWindowProperties().get('powershell_response');
  var xmldoc = new XMLDocument(Packages.com.glide.util.StringUtil.unEscapeHTML(rString));
  var output = xmldoc.getNodeText("//response/output");
  output = Packages.com.glide.util.StringUtil.base64Decode(output);
  output;
</g:evaluate>

<g:evaluate var="jvar_error">
  var rString = RP.getWindowProperties().get('powershell_response');
  var xmldoc = new XMLDocument(Packages.com.glide.util.StringUtil.unEscapeHTML(rString));
  var error = xmldoc.getNodeText("//response/error");
  error = Packages.com.glide.util.StringUtil.base64Decode(error);
  error;
</g:evaluate>

<div style='width:100%;background-color:#A8A4BF;'>

<div style='margin-left: auto; margin-right: auto;'>
  <textarea style='font-size: 1em; background-color: black; color: white; width: 600px; height: 20em; padding: 5px; margin-left:auto; margin-right:auto' readonly='readonly'>${jvar_output}</textarea>

<j2:if test="${jvar_error!='' &amp;&amp; jvar_error!='null'}">
  <div style='font-size: 1.2em; font-weight: bold; color: black;margin-left:auto; margin-right:auto'>Error</div>
  <textarea style='font-size: 1em; background-color: #BB3333; color: white; width: 600px; height: 10em; padding: 5px; margin-left:auto; margin-right:auto' readonly='readonly'>${jvar_error}</textarea>
</j2:if>
